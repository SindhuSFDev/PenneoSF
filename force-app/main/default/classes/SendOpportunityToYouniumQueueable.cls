public class SendOpportunityToYouniumQueueable implements Queueable,Database.AllowsCallouts{
    public List<Opportunity> oppts;
    public SendOpportunityToYouniumQueueable(List<Opportunity> opptList){
        this.oppts = opptList;
    }
    
    public void execute(QueueableContext context) {
        Opportunity singleOpp = oppts[0];
        try{
            system.debug('singleOpp: '+singleOpp);
            if(!Test.isRunningTest()){
                Younium.CreateOrderData.createOrderData(new List<Opportunity>{singleOpp});
            }
            
        }catch(Exception e){
            System.debug('exception line - '+e.getLineNumber()+ ' message - '+e.getMessage());
            singleOpp.Sync_Status__c = 'exception line - '+e.getLineNumber()+ ' message - '+e.getMessage();
            update singleOpp;
        }finally{
            oppts.remove(0);
            if(!oppts.isEmpty() && !Test.isRunningTest()){
                SendOpportunityToYouniumQueueable syncOpp = new SendOpportunityToYouniumQueueable(oppts);
                system.enqueueJob(syncOpp);
            }
        }
        
        
    }
