    @future
    public static void updateYouniumOpps(Set<String> oppsUpdateIds){
        List<Opportunity> oppsUpdate = [SELECT Id, Younium__Is_Synched_To_Younium__c FROM Opportunity WHERE Id IN : oppsUpdateIds];
        for(Opportunity opp : oppsUpdate){
            opp.Younium__Is_Synched_To_Younium__c = true;
        }
        if(!oppsUpdate.isEmpty()){
            OpportunityTriggerHandler obj = new OpportunityTriggerHandler();
            obj.IsDisabled();
            OpportunityTriggerHandler.checkRecursion = true;
            update oppsUpdate;
        }
    }
    
    public void updatePrimaryContactOnOpportunity(Map<Id, Opportunity> oldMap, Map<Id, Opportunity> newMap) {
        Set<String> oppIdSet1 = new Set<String>();
        Set<String> oppIdSet2 = new Set<String>();
        Set<String> oppIdSet3 = new Set<String>();
        Set<String> primaryOppIds = new Set<String>();
        
        //get primary OCR
        List<OpportunityContactRole> primaryOCR = [SELECT Id, OpportunityId FROM OpportunityContactRole WHERE OpportunityId IN:newMap.keySet() AND IsPrimary = true];
        if (!primaryOCR.isEmpty()) {
            for (OpportunityContactRole ocr : primaryOCR) {
                primaryOppIds.add(ocr.OpportunityId);
            }
        }
        
        //query contact roles
        List<AggregateResult> oppContRole = [
            SELECT COUNT(Id) ocrcount, OpportunityId
            FROM OpportunityContactRole
            WHERE OpportunityId IN:newMap.keySet() AND IsPrimary = false AND OpportunityId NOT IN :primaryOppIds
            GROUP BY OpportunityId
        ];
        if (oppContRole != null && !oppContRole.isEmpty()) {
            for (AggregateResult aggr : oppContRole) {
                Id oppId = (Id) aggr.get('OpportunityId');
                if (oppId != null && (Integer) aggr.get('ocrcount') == 1) {
                    oppIdSet1.add(oppId);
                }
                if (oppId != null && (Integer) aggr.get('ocrcount') > 1) {
                    oppIdSet2.add(oppId);
                }
            }
        }
        else{
            List<Opportunity> opps = [SELECT Id, AccountId FROM Opportunity WHERE Id IN :newMap.keySet() AND Id NOT IN :primaryOppIds];
            if (!opps.isEmpty()) {
                
                Set<String> accountIds = new Set<String>();
                Set<String> contactIds = new Set<String>();
                Map<String, String> mapOfContIdVsOCRId = new Map<String, String>();
                List<OpportunityContactRole> ocrToUpdate = new List<OpportunityContactRole>();
                Set<String> contactIdsToUp = new Set<String>();
                Map<String, String> mapOfAccIdVsContId = new Map<String, String>();
                Map<String, String> mapOfAccIdVsOppId = new Map<String, String>();
                List<OpportunityContactRole> ocrInsertList = new List<OpportunityContactRole>();
                
                for (Opportunity o : opps) {
                    accountIds.add(o.AccountId);
                    mapOfAccIdVsOppId.put(o.AccountId, o.Id);
                }
                
                //query activities of contact
                List<Contact> contacts = [SELECT Id, AccountId, (SELECT Id, CreatedDate FROM ActivityHistories ORDER BY CreatedDate DESC) FROM Contact WHERE AccountId IN :accountIds];
                for (Contact cont : contacts) {
                    if (!cont.ActivityHistories.isEmpty()) {
                        Boolean hasActivity = false;
                        for (ActivityHistory a : cont.ActivityHistories) {
                            if (!hasActivity) {
                                contactIdsToUp.add(cont.Id);
                                hasActivity = true;
                                mapOfAccIdVsContId.put(cont.AccountId, cont.Id);
                            }
                        }
                    }
                }
                
                for(String accId : mapOfAccIdVsContId.keyset()){
                    OpportunityContactRole ocr = new OpportunityContactRole();
                    ocr.ContactId = mapOfAccIdVsContId.get(accId);
                    ocr.OpportunityId = mapOfAccIdVsOppId.get(accId);
                    ocr.IsPrimary = true;
                    ocrInsertList.add(ocr);
                }
                if(!ocrInsertList.isEmpty()){
                    System.debug('ocrInsertList: '+ocrInsertList);
                    insert ocrInsertList;
                }
            }
        }
        
        //Mark primary when one OCR
        List<OpportunityContactRole> updateOCRPrimarySet1 = [SELECT Id, IsPrimary FROM OpportunityContactRole WHERE OpportunityId IN :oppIdSet1];
        if (!updateOCRPrimarySet1.isEmpty()) {
            for (OpportunityContactRole ocr : updateOCRPrimarySet1) {
                ocr.IsPrimary = true;
            }
            System.debug('ocrToUpdate1: ' + updateOCRPrimarySet1);
            update updateOCRPrimarySet1;
        }
        
        //Mark primary when more than one OCR
        List<OpportunityContactRole> updateOCRPrimarySet3 = [SELECT Id, IsPrimary, ContactId, OpportunityId FROM OpportunityContactRole WHERE OpportunityId IN :oppIdSet2];
        if (!updateOCRPrimarySet3.isEmpty()) {
            
            Set<String> contactIds = new Set<String>();
            Map<String, String> mapOfContIdVsOCRId = new Map<String, String>();
            List<OpportunityContactRole> ocrToUpdate = new List<OpportunityContactRole>();
            Set<String> contactIdsToUp = new Set<String>();
            
            for (OpportunityContactRole ocr : updateOCRPrimarySet3) {
                contactIds.add(ocr.ContactId);
                mapOfContIdVsOCRId.put(ocr.ContactId, ocr.Id);
            }
            
            //query activities of contact
            List<Contact> contacts = [SELECT Id, (SELECT Id, CreatedDate FROM ActivityHistories ORDER BY CreatedDate DESC) FROM Contact WHERE Id IN :contactIds];
            for (Contact cont : contacts) {
                if (!cont.ActivityHistories.isEmpty()) {
                    Boolean hasActivity = false;
                    for (ActivityHistory a : cont.ActivityHistories) {
                        if (!hasActivity) {
                            contactIdsToUp.add(cont.Id);
                            hasActivity = true;
                        }
                    }
                }
            }
            if (!contactIdsToUp.isEmpty()) {
                for (String contId : contactIdsToUp) {
                    OpportunityContactRole ocrUp = new OpportunityContactRole();
                    ocrUp.Id = mapOfContIdVsOCRId.get(contId);
                    ocrUp.IsPrimary = true;
                    ocrToUpdate.add(ocrUp);
                }
            }
            
            if (!ocrToUpdate.isEmpty()) {
                System.debug('ocrToUpdate2: ' + ocrToUpdate);
                update ocrToUpdate;
            }
        }
    }
    
