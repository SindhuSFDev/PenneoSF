        Map<Id, Opportunity> oldmapopp = (Map<Id, Opportunity>) oldMap;
        Map<Id, Opportunity> closedWonOpps = new Map<Id, Opportunity>();
        for (Id oppId : newmapopp.keySet()) {
            if ((newmapopp.get(oppId).StageName == 'Closed Won' || newmapopp.get(oppId).StageName == 'Closed Lost') && oldmapopp.get(oppId).StageName != newmapopp.get(oppId).StageName) {
                closedWonOpps.put(oppId, newmapopp.get(oppId));
            }
        }
        if(!checkRecursion){
            updatePrimaryContactOnOpportunity(oldmapopp, closedWonOpps);
        }
    }
    
